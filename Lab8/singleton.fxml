<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MyController">
    <GridPane prefHeight="480.0" prefWidth="640.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="240.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Button fx:id="button1" onAction="#selectFile" alignment="CENTER" prefHeight="1000" prefWidth="1000" text="Read first.txt" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <Label fx:id="label1" alignment="CENTER" prefHeight="1000" prefWidth="1000" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <Button fx:id="button2" onAction="#selectFile" alignment="CENTER" prefHeight="1000" prefWidth="1000" text="Read second.txt" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <Label fx:id="label2" alignment="CENTER" prefHeight="1000" prefWidth="1000" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="button3" onAction="#selectFile" alignment="CENTER" prefHeight="1000" prefWidth="1000" text="Read third.txt" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
            <Label fx:id="label3" alignment="CENTER" prefHeight="1000" prefWidth="1000" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        </children>
    </GridPane>
</Pane>
